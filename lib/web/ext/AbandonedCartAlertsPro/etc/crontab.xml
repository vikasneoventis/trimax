<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Magento/Cron/etc/crontab.xsd">
    <group id="default">
        <job name="aitoc_abandoned_cart_alerts_pro_generate_alerts" instance="Aitoc\AbandonedCartAlertsPro\Model\Cron" method="generateQuoteAlerts">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="aitoc_abandoned_cart_alerts_pro_send_alerts" instance="Aitoc\AbandonedCartAlertsPro\Model\Cron" method="processAlerts">
            <schedule>*/5 * * * *</schedule>
        </job>
    </group>
</config>
