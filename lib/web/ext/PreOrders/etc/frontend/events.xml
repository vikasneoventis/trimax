<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="catalog_product_is_salable_after">
        <observer name="aitoc_preorder_salable_observer" instance="Aitoc\PreOrders\Observer\AfterSalableProduct" />
    </event>
    <event name="sales_quote_item_qty_set_after">
        <observer name="aitoc_preorder_after_set_qty_item_quote" instance="Aitoc\PreOrders\Observer\AfterSetQtyItemQuote" />
    </event>
    <event name="checkout_cart_product_add_after">
        <observer name="aitoc_preorder_add_product_after" instance="Aitoc\PreOrders\Observer\CheckQuoteProduct" />
    </event>
</config>
